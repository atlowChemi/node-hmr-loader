{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 4],
    "capitalized-comments": ["error", "always", { "ignoreConsecutiveComments": true }],
    "strict": ["error", "safe"]
  }
}
